{
  "name": "faciliauto-mvp",
  "version": "1.0.2",
  "description": "MVP - Assistente de Vendas WhatsApp com IA para ConcessionÃ¡rias",
  "main": "dist/index.js",
  "scripts": {
    "heroku-prebuild": "echo 'Skipping TypeScript build, using tsx directly'",
    "heroku-postbuild": "npx prisma generate && npm run db:seed",
    "heroku-cleanup": "echo 'Build completed successfully!'",
    "start:heroku": "npx tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "dev:api": "tsx watch src/api-test-server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "tsx src/index.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:seed:renatinhu": "tsx src/scripts/seed-renatinhu.ts",
    "db:seed:complete": "tsx src/scripts/seed-renatinhu-complete.ts",
    "db:seed:real": "tsx src/scripts/seed-renatinhu-real.ts",
    "db:seed:robustcar": "tsx prisma/seed-robustcar.ts",
    "db:migrate": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "vitest run tests/e2e",
    "test:integration": "vitest run tests/integration",
    "test:integration:llm": "vitest run tests/integration/llm-integration.test.ts",
    "test:unit": "vitest run tests/unit",
    "test:smoke": "vitest run tests/integration/llm-integration.test.ts --testNamePattern='Smoke|Availability'",
    "test:bot": "tsx src/test-bot.ts",
    "test:guardrails": "tsx src/test-guardrails.ts",
    "test:new-number": "tsx scripts/test-new-whatsapp-number.ts",
    "embeddings:generate": "tsx src/scripts/generate-embeddings.ts generate",
    "embeddings:regenerate": "tsx src/scripts/generate-embeddings.ts regenerate",
    "embeddings:stats": "tsx src/scripts/generate-embeddings.ts stats",
    "embeddings:force": "tsx src/scripts/generate-embeddings.ts generate force",
    "benchmark:llms": "tsx src/scripts/benchmark-llms.ts",
    "conversations:reset": "tsx scripts/reset-conversations.ts",
    "conversations:reset:all": "tsx scripts/reset-conversations.ts --all",
    "vehicles:update-uber": "tsx scripts/update-uber-eligibility.ts",
    "vehicles:update-uber-llm": "tsx scripts/update-uber-eligibility-llm.ts",
    "uber:scrape": "tsx src/scripts/scrape-uber-rules.ts",
    "uber:batch": "tsx src/scripts/uber-rules-batch.ts",
    "heroku-status": "heroku ps --app faciliauto-mvp",
    "heroku-logs": "heroku logs --tail --app faciliauto-mvp",
    "heroku-open": "heroku open --app faciliauto-mvp",
    "lint": "eslint src/**/*.ts --max-warnings 0",
    "lint:warn": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "verify": "npm run format && npm run lint:warn && npm run test:unit",
    "verify:strict": "npm run format:check && npm run lint && npm run test:run",
    "push:safe": "npm run verify && git push origin main && git push novais main",
    "push:origin": "npm run verify && git push origin main",
    "push:novais": "npm run verify && git push novais main",
    "benchmark": "tsx src/evaluation/run-benchmark.ts",
    "benchmark:single": "tsx -e \"require('./src/evaluation/benchmark-runner').benchmarkRunner.runSingle(process.argv[2]).then(r => console.log(JSON.stringify(r, null, 2)))\""
  },
  "keywords": [
    "whatsapp",
    "ai",
    "carros",
    "vendas"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^1.1.24",
    "@langchain/langgraph": "^1.1.4",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "@langchain/openai": "^1.2.7",
    "@prisma/client": "^5.7.0",
    "@types/string-similarity": "^4.0.2",
    "axios": "^1.13.5",
    "cohere-ai": "^7.20.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.37.0",
    "langchain": "^1.2.24",
    "langsmith": "^0.5.4",
    "openai": "^6.9.0",
    "pino": "^8.16.2",
    "qrcode-terminal": "^0.12.0",
    "string-similarity": "^4.0.4",
    "tsx": "^4.21.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.10",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "fast-check": "^4.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "prisma": "^5.7.0",
    "supertest": "^7.2.2",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.10"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,ts,md,json,txt}": [
      "bash -c 'if grep -rE \"(gsk_[a-zA-Z0-9]{20,}|EAA[a-zA-Z0-9]{100,})\" --include=*.js --include=*.ts --include=*.md --include=*.json .; then exit 1; fi'"
    ]
  }
}
